{
  "name": "vue_mobild_project_template",
  "version": "0.0.1",
  "description": "vue移动端项目模板",
  "author": "SeekerGAO",
  "private": true,
  "scripts": {
    "start:local": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env local",
    "start:dev": "webpack-dev-server --inline --progress --host 0.0.0.0 --config build/webpack.dev.conf.js --env dev",
    "start:prod": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env prod",
    "start:dev-pre": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env dev-pre",
    "start:prod-pre": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env prod-pre",
    "build:dev": "node build/build.js --env dev",
    "build:prod": "node build/build.js --env prod",
    "build:dev-pre": "node build/build.js --env dev-pre",
    "build:prod-pre": "node build/build.js --env prod-pre",
    "serve:dev:": "serve ./dist/dev",
    "serve:prod": "serve ./dist/prod",
    "serve:dev-pre": "serve ./dist/dev-pre",
    "serve:prod-pre": "serve ./dist/prod-pre",
    "test": "mocha-webpack --webpack-config build/webpack.base.conf.js --require test/setup.js test/**/*.spec.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "reset-css": "^5.0.1",
